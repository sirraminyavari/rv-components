  "scripts": {
    "build": "yarn build:clear-dist && yarn build:tsc && yarn build:rollup && yarn build:scss",
    "build:clear-dist": "rm -rf dist",
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "build:tsc": "tsc -p ./tsconfig.json",
    "build:scss": "cpx 'src/**/*.{scss,css,ttf,eot,woff,woff2,svg}' dist",
    "postbuild": "cpx './package.json' dist",
    "lint": "eslint src --color --fix",
    "stylint": "stylelint 'src/**/*.{scss,css}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "storybook": "start-storybook -p 6006  --no-manager-cache",
    "storybook:build": "build-storybook",
    "storybook:chromatic": "chromatic --exit-zero-on-changes",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.8",
