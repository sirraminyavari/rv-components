{
  "name": "@cliqmind/rv-components",
  "private": false,
  "version": "0.0.0-dev",
  "scripts": {
    "dev": "vite",
    "build": "yarn run clean:dist && yarn run export:lib && yarn run export:pkgJson && yarn run export:fix-structure",
    "export:lib": "tsc && vite build",
    "export:fix-structure": "./afterBuild.sh",
    "export:pkgJson": "cp package.json ./dist/package.json",
    "clean:dist": "rimraf dist",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "lint": "eslint \"src/lib/**/*.{ts,tsx,css,scss,json}\"",
    "lint:fix": "eslint \"src/lib/**/*.{ts,tsx,css,scss,json}\" --fix",
    "clean:deps": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "prepack": ""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn run lint",
      "yarn run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@cliqmind/react-table": "^7.8.0-dev.1",
    "classnames": "^2.3.1",
    "react": "^16.8.0 || 17.x",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^16.8.0 || 17.x",
    "react-icons": "^4.4.0",
    "react-table-sticky": "^1.1.3",
    "styled-components": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "^17.0.26",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-table": "^7.7.12",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.0",
    "acorn-jsx": "^5.3.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "json": "^11.0.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "typescript": "4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-dts": "^1.1.0"
  },
  "files": [
    "components/",
    "constant/",
    "contexts/",
    "helpers/",
    "hooks/",
    "utilities/",
    "index.d.ts",
    "index.js"
  ],
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts"
}
